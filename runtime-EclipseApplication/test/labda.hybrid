specification Labda {
	const D : real := 0.1
	const K : real := 0.95
	const g : real := 9.8
	
	property safe : {
		local x : real //[0, 15] x;
		local v : real //[-18, 18] v;
		//[0, 3] time;
		
		location down {
			initial x = 5
			initial v = 0
			flow d(x) = v
			flow d(v) = -g + D * v * v
			invariant v <= 0
			invariant x >= 0
		}
		
		location up {
			flow d(x) = v
			flow d(v) = -g - D * v * v			
			invariant v >= 0
			invariant x >= 0
		}
		
		transition down to up [x <= 1] {x := x, v := -K * v}
		transition up to down [x = 1] {x := x, v := v}		
		
	} models G(@up imply v <= 10)
	
}